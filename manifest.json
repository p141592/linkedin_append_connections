{
    "name": "k0d LinkedIn",
    "description": "LinkedIn Expansions",
    "version": "0.0.4",
    "permissions": [
        "<all_urls>",
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["https://www.linkedin.com/mynetwork/"],
        "js": ["src/js/background.js"],
        "run_at": "document_end"
    },{
        "matches": ["https://www.linkedin.com/mynetwork/"],
        "js": ["static/library/jquery.min.js"],
        "run_at": "document_start"
    },{
        "matches": ["https://www.linkedin.com/mynetwork/"],
        "js": ["static/library/utils.js"],
        "run_at": "document_start"
    }
    ],
    "icons": {
        "128": "src/img/logo-128.jpg"
    },
    "browser_action": {
        "default_title": "k0d.ru LN extension",
        "default_icon": {
            "128": "src/img/logo-128.jpg"
        },
        "default_popup": "src/html/popup.html"
    },
    "manifest_version": 2,
    "author": "Baryshnikov Nikolay"
}
