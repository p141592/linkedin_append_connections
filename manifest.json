{
    "name": "k0d LinkedIn",
    "description": "Expansions",
    "version": "0.0.1",
    "permissions": [
        "<all_urls>"
    ],
    "background": {
        "scripts": ["src/js/popup.js"],
        "persistent": true
    },
    "icons": {
        "128": "src/img/logo-128.jpg"
    },
    "browser_action": {
        "default_title": "k0d.ru LN extension",
        "default_icon": {
            "128": "src/img/logo-128.jpg"
        },
        "default_popup": "src/html/popup.html"
    },
    "manifest_version": 2,
    "author": "Baryshnikov Nikolay"
}
